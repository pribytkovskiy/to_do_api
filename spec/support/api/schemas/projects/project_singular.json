{
  "id": "file:/project_singular.json#",
  "type": "object",
  "required": ["id", "type", "attributes", "relationships", "links"],
  "properties": {
    "id": { "type": "string" },
    "type": { "const": "projects" },
    "attributes": {
      "type": "object",
      "required": ["name"],
      "properties": {
        "name": { "type": "string" }
      }
    },
    "relationships": {
      "type": "object",
      "required": ["tasks", "user"],
      "properties": {
        "tasks": {
          "type": "object",
          "required": ["data"],
          "properties": {
            "data": {
              "type": "array",
              "items": { "$ref": "file:/task_identifier_object.json#" }
            }
          }
        },
        "user": {
          "type": "object",
          "required": ["data"],
          "properties": {
            "data": { "$ref": "file:/user_identifier_object.json#" }
          }
        }
      }
    },
    "links": {
      "type": "object",
      "properties": {
        "self": { "type": "string" }
      }
    }
  }
}